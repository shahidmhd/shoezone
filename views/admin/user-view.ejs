

 <div class="page-wrapper">
    <div class="main-content">
      <div class="section__content section__content--p30">
          <div class="container-fluid">
            <div class="row">
                <div class="col-md-6">
                  <div class="overview-wrap">
                      <h2 class="title-1">USER DETAILS</h2>
                  </div>
                </div>
            </div>
              <div class="row mt-5">
                  <div class="col-lg-12">
                      <div class="table-responsive table--no-card m-b-30">
                          <table class="table table-borderless table-striped table-earning" id="productsTable">
                            <thead>
                              <tr>
                                <th>Index</th>
                                <th>username</th>
                                <th>email</th>
                                <th class="text-right">phone</th>
                                <th class="text-right">option</th>
                                
                              </tr>
                            </thead>
                            <tbody>
                                  <% for(var i=0; i < users.length; i++) { %>
                                    <tr>
                                        <td><%=i+1%></td>
                                        <td><%= users[i].name %></td>
                                        <td><%= users[i].email %></td>
                                        <td class="text-right"><%= users[i].Phonenumber %></td>
                                        <td class="text-right">
                                          <% if(users[i].isBlocked){%>
                                            <a href="/admin/block/<%=users[i]._id%>" class="btn btn-success">unblock</a>
                                         <% }else{%>
                                          <a href="/admin/block/<%=users[i]._id%>" class="btn btn-danger">block</a>
                                         <%}%>
                                        </td>
                                    </tr>
                                    <% } %>
                                </tbody>
                          </table>
                      </div>
                  </div>
              </div>

              <div class="row m-t-30">
              </div>
          </div>
      </div>
  </div>
  </div>


  <script>

    $(function () {
      $('#productsTable').DataTable();
    });
    </script>
     