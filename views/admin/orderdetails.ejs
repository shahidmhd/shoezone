                                      <li class="text-muted"><i class="fa fa-circle" style="color:#84B0CA ;"></i> <span
                                          class="fw-bold">Email:</span><%=orders[0].useremail%></li>
                                      <li class="text-muted"><i class="fa fa-circle" style="color:#84B0CA ;"></i> <span
                                          class="fw-bold">Phone:</span><%=orders[0].userphonenumber%></li>
                                      <li class="text-muted"><i class="fa fa-circle" style="color:#84B0CA ;"></i> <span
                                          class="fw-bold">Date: </span><%=orders[0].date%></li>
                                      <li class="text-muted"><i class="fa fa-circle" style="color:#84B0CA ;"></i> <span
                                          class="me-1 fw-bold">Status:</span><span class="badge bg-warning text-black fw-bold">
                                              <%=orders[0].status%></span></li>
                                    </ul>
                                  </div>
                                </div>
                        
                                <div class="row my-2 mx-1 justify-content-center">
                                  <table class="table">
                                      <thead class="thead-dark">
                                        <tr>
                                          <th scope="col">index</th>
                                          <th scope="col">name</th>
                                          <th scope="col">quantity</th>
                                          <th scope="col">price</th>
                                          <th scope="col">COP_offer</th>
                                          <th scope="col">image</th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                        <% for(let i=0;i< orders.length;i++){%>
                                        <tr>
                                          <td><%=i+1%></td>
                                          <td><%=orders[i].productdetails.productname%></td>
                                          <td><%=orders[i].products.quantity%></td>
                                          <td><%=orders[i].productdetails.price%></td>
                                          <td><%=orders[i].offer%></td>
                                          <td><img src="<%=orders[i].productdetails.imagesurl[0]%>" style="height: 50px;width: 50px;"></td>
                                        </tr>
                                        <%}%>
                                      </tbody>
                                    </table>
                                </div>
                                <div class="row">
                                  <div class="col-xl-8">
                                   
                                  </div>
                                  <div class="col-xl-3">
                                    <ul class="list-unstyled">
                                      
                                    </ul>
                                    <p class="text-black float-start"><span class="text-black me-3"> Total Amount:</span><span
                                        style="font-size: 25px;"><%=orders[0].total%></span></p>
                                  </div>
                                </div>
